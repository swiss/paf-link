
@prefix : <https://example.com/> .
@prefix paf: <https://paf.link/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix schema: <http://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterm: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:registration-activity a paf:RegistrationActivity;
    prov:startedAtTime "2024-01-01T12:00:00Z"^^xsd:dateTime.

:identifier-entity a prov:Entity;
    prov:wasGeneratedBy :registration-activity;
    schema:identifier "order_1".

:order-creation-activity a paf:OrderCreationActivity;
    prov:wasInformedBy :registration-activity;
    prov:startedAtTime "2024-01-02T12:00:00Z"^^xsd:dateTime;
    prov:used :identifier-entity.

:order-entity a prov:Entity;
    prov:wasGeneratedBy :order-creation-activity;
    schema:description "They shall do: ..."@en.

:order-activity a paf:OrderActivity;
    prov:wasInformedBy :order-creation-activity;
    prov:startedAtTime "2024-01-03T12:00:00Z"^^xsd:dateTime;
    prov:used :identifier-entity, :order-entity;
    prov:qualifiedAssociation [ 
        a prov:Association; 
        prov:agent :federal-council;;
        prov:hadRole paf:OrderSubmitter];
    prov:qualifiedAssociation [
        a prov:Association;
        prov:agent :FOEN;
        prov:hadRole paf:OrderReceiver].

:completion-creation-activity a paf:CompletionCreationActivity;
    prov:wasInformedBy :order-activity;
    prov:startedAtTime "2024-01-04T12:00:00Z"^^xsd:dateTime;
    prov:used :order-entity.

:oder-fulfillment-entity a prov:Entity;
    prov:wasGeneratedBy :completion-creation-activity;
    schema:description "The order has been fulfilled by doing: ..."@en.

:completion-activity a paf:CompletionActivity;
    prov:wasInformedBy :completion-creation-activity;
    prov:startedAtTime "2024-01-05T12:00:00Z"^^xsd:dateTime;
    prov:used :identifier-entity, :order-entity, :oder-fulfillment-entity;
    prov:qualifiedAssociation [
        a prov:Association;
        prov:agent :FOEN-forest;
        prov:hadRole paf:Completer].